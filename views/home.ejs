<%- include("partials/header"); -%>

<h1>Home</h1>
<p>
    <%= homeStartingContent %>
</p>
<% posts.forEach((post) => { %>
    <div class="post_container">
        <h2>
            <%= post.title %>
        </h2>
        <p>
            <%= post.title.substring(0, 100) + "..." %>
            <%= post.content %>
            <a href="/posts/<%=post._id%>">Read More</a>
        </p>
    </div>
<% }) %>


<%- include("partials/footer"); -%>