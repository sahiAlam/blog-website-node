<%- include("partials/header"); -%>

<h1>Home</h1>
<p>
    <%= homeStartingContent %>
</p>
<% posts.forEach((post) => { %>
    <div>
        <h2>
            <%= post.composeText %>
        </h2>
        <p>
            <%= post.composePost.substring(0, 100) + "..." %>
            <a href="/posts/<%=post.composeText%>">Read More</a>
        </p>
    </div>
<% }) %>


<%- include("partials/footer"); -%>